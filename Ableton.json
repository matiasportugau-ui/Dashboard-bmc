{
    "version": "2025-10-22",
    "modules": [
        {
            "id": "eq-eight",
            "name": "EQ Eight",
            "category": "equalizer",
            "device": "EQ Eight",
            "recommended_settings": {
                "low_cut_hz": 30,
                "high_shelf_hz": 14500,
                "dynamic_notches": [
                    {"freq_hz": 250, "gain_db": -2.0, "q": 1.0},
                    {"freq_hz": 3500, "gain_db": -1.5, "q": 0.7}
                ]
            },
            "notes": "Surgical cleanup and tonal balancing; enable oversampling when using on the master bus."
        },
        {
            "id": "multiband-dynamics",
            "name": "Multiband Dynamics",
            "category": "dynamics",
            "device": "Multiband Dynamics",
            "recommended_settings": {
                "low_band_range_db": [-3.5, 1.5],
                "mid_band_range_db": [-2.5, 1.5],
                "high_band_range_db": [-1.5, 0.5],
                "crossover_hz": [120, 5500]
            },
            "notes": "Primary control for energy distribution; use gentle ratios and slow release for transparent mastering."
        },
        {
            "id": "glue-compressor",
            "name": "Glue Compressor",
            "category": "dynamics",
            "device": "Glue Compressor",
            "recommended_settings": {
                "attack_ms": 3.0,
                "release_mode": "auto",
                "ratio": 2.0,
                "make_up_db": 1.5
            },
            "notes": "Bus compression to unify the mix; keep gain reduction below 2 dB for mastering contexts."
        },
        {
            "id": "saturator",
            "name": "Saturator",
            "category": "color",
            "device": "Saturator",
            "recommended_settings": {
                "mode": "analog_clip",
                "drive_db": 1.5,
                "dry_wet": 15
            },
            "notes": "Add subtle harmonic excitement after compression; keep dry/wet under 20% to avoid audible distortion."
        },
        {
            "id": "utility",
            "name": "Utility",
            "category": "utility",
            "device": "Utility",
            "recommended_settings": {
                "stereo_width_percent": 115,
                "mono_below_hz": 120,
                "gain_db": 0.0
            },
            "notes": "Final gain staging and stereo image control; collapse lows to mono for tighter bass."
        },
        {
            "id": "limiter",
            "name": "Limiter",
            "category": "limiter",
            "device": "Limiter",
            "recommended_settings": {
                "ceiling_db": -0.8,
                "lookahead_ms": 1.0,
                "release_ms": 100
            },
            "notes": "Brick-wall limiting at the end of the chain; adapt ceiling per delivery platform."
        }
    ],
    "effects": [
        {
