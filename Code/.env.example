# Configuración de APIs
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Configuración de Cursor
CURSOR_WORKSPACE_PATH=/path/to/cursor/workspace
CURSOR_PROJECT_ID=your_project_id

# Configuración de GPT
GPT_MODEL=gpt-4-turbo-preview
GPT_TEMPERATURE=0.7
GPT_MAX_TOKENS=4000

# Configuración de sincronización
SYNC_INTERVAL_MS=30000
AUTO_SYNC_ENABLED=true

# Configuración de desarrollo
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Configuración de automatización
AUTOMATION_ENABLED=true
MAX_CONCURRENT_TASKS=5
TASK_TIMEOUT_MS=300000

# Configuración de base de datos (opcional)
DATABASE_URL=postgresql://user:password@localhost:5432/code_dev

# Configuración de webhooks
WEBHOOK_SECRET=your_webhook_secret
CURSOR_WEBHOOK_URL=http://localhost:3001/webhook
GPT_WEBHOOK_URL=http://localhost:3002/webhook