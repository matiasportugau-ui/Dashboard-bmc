{
  "name": "code-env",
  "version": "1.0.0",
  "description": "Entorno de desarrollo automatizado compatible con AI Assistant",
  "main": "index.js",
  "scripts": {
    "setup": "node scripts/setup/init.js",
    "create:project": "node scripts/automation/create-project.js",
    "deploy": "node scripts/deployment/deploy.js",
    "ai:sync": "node scripts/ai-communication/sync.js",
    "lint": "eslint . --ext .js,.ts,.tsx,.jsx",
    "format": "prettier --write .",
