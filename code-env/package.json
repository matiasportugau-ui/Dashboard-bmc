  "devDependencies": {
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "webpack": "^5.88.2",
